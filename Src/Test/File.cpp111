#include "Poco/File.h"
#include "Poco/FileStream.h"
#include <iostream>
using namespace Poco;
int main(int argc, char** argv)
{
	File file("./user.info");
	FileStream fs("./user.info");
	char m[32] = {0,};
	char user[16];
	char passwd[16];
	while(fs.peek() != EOF)
	{
		fs.getline(m, 32);
		std::cout << m << std::endl;
		sscanf(m, "%s %s", user, passwd);
		std::cout << "user:" << user << " pass:" << passwd << std::endl;
	}
	return 0;
}
